@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
}

<h3 class="text-danger">An error occurred while processing your request.</h3>

<p>
    <strong>Request ID:</strong> <code>@Model.RequestId</code>
    <br />
    <strong>Exception Message:</strong> <code>@Model.ExceptionMessage</code>
    <br />
    <strong>Exception Stack Trace:</strong> <code>@Model.ExceptionStackTrace</code>
</p>
